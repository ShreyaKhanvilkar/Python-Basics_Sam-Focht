def factors(x):
    d = []
    for i in range(2,x+1):
        if x % i == 0:
            d.append(i)
    return d

print(factors(10))
'''


## Episode Ninety-Five
'''
def factors(x):
    d = [i for i in range(1,x+1) if x % i == 0]
    return d

for i in factors(18):
    print(i)
'''


## Episode Ninety-Six
'''
def factors(x):
    d = [i for i in range(x+1,0,-1) if x % i == 0]
    return d
num = [81,18]
for i in factors(max(num)):
    if i in factors(min(num)):
        print(i)
        break
'''


## Episode Ninety-Seven
'''
def factors(x):
    d = [i for i in range(x+1,0,-1) if x % i == 0]
    return d
num = [81,18]
print(f"Finding the GCF for: {num[0]} and {num[1]}")
for i in factors(min(num)):
    if i in factors(max(num)):
        print(f"Test: {i} - Match")
        break
    else:
        print(f"Test: {i} - Fail")
'''


## Episode Ninety-Eight
'''
def gcd(x,y):
    com = min(x,y)
    while x % com != 0 or y % com != 0:
        com -= 1
    return com
print(gcd(81,18))
'''


