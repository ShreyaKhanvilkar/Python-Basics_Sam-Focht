## Episode Thirty-Nine
'''
def time_table():
    while True:
        try:
            x = int(input("Enter a number: "))
            for i in range(1,x+1):
                for j in range(1,x+1):
                    print(f"{i*j:3}", end="")
                print()
            print()
            q = input("Would you like another table? (y/n): ").lower()[0]
            if q == "n":
                break
        except ValueError:
            print("Please enter a number.")
            continue

time_table()
'''


## Episode Fourty
'''
def leap_year():
    while True:
        try:
            x = int(input("Enter a year: "))
            if x % 4 == 0:
                if x % 100 == 0:
                    if x % 400 == 0:
                        print(x, "is a leap year.")
                    else:
                        print(x, "is not a leap year.")
                else:
                    print(x, "is a leap year.")
            else:
                print(x, "is not a leap year.")
            q = input("Would you like to test another year? (y/n): ").lower()[0]
            if q == "n":
                break
        except ValueError:
            print("Please enter a year.")

leap_year()
'''


## Episode Fourty-One
'''
x = "+ "
y = "- " * 5

def bottom():
    print((x + y) * 2 + x)

def middle():
    for i in range(4):
        print(("|           "*3))

def box():
    bottom()
    middle()

def grid():
    box()
    box()
    bottom()
    
grid()
'''


## Episode Fourty-Two
'''
def prime(x):
    for i in range(2,x):
        if x % i == 0:
            return False
    return True

def nth_prime(x):
    num = 3
    prime = 2
    if x == 1:
        return 2
    while prime < x:
        num += 2
        if prime(num):
            prime += 1
        return num
print(nth_prime(1000))
'''


## Episode Fourty-Three
'''
def dia(x):
    for i in range(x,-1,-1):
        print(" " * i, end = "")
        for j in range(x+1-i):
            print(j, end = "")
        for a in range(x-i-1,-1,-1):
            print(a, end = "")
        print()
    for b in range(1,x+1):
        print(" " * b, end = "")
        for c in range(x+1-b):
            print(c, end = "")
        for d in range(c-1,-1,-1):
            print(d, end = "")
        print()
dia(9)
'''


## Episode Fourty-Four
'''
s,n,t = 6,9,20
def nuggets(y):
    global x
    x = {}
    for i in range(10):
        for j in range(10):
            for k in range(10):
                tot = k*s+j*n+i*t
                x[tot] = [k,j,i]
                if y == tot:
                    break
            if y == tot:
                break
        if y == tot:
            break
    print(y,x[y])

nuggets(50)
'''



## Episode Fourty-Five
'''
def sec(x):
    d = x//86400
    h = (x%86400)//3600
    m = (x%3600)//60
    s = (x%60)
    print(F"{d}:{h}:{m}:{s}")

print("d:h:m:s")

sec(134523423)
'''


## Episode Fourty-Six
'''
def fact(x):
    if x == 1:
        return 1
    else:
        return x * fact(x-1)

print(fact(40))

