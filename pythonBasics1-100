## Episode Fifty-Five
'''
import random
number = random.randint(1,20)
guess = int(input("Make a guess: "))

while guess != number:
    if guess > number:
        print("Please guess a lower number.")
        guess = int(input("Make a guess: "))
    else:
        print("Please guess a higher number.")
        guess = int(input("Make a guess: "))
else:
    print("Your guess is correct.")
'''


## Episode Fifty-Six
'''
import random
number = random.randint(1,20)
print(number)
while True:
        try:
            guess = int(input("Make a guess: "))
            while guess != number:
                if guess > number:
                    print("Please guess a lower number.")
                    guess = int(input("Make a guess: "))
                else:
                    print("Please guess a higher number.")
                    guess = int(input("Make a guess: "))
            else:
                print("Your guess is correct.")
                break
        except ValueError:
            print("Please enter a number.")
